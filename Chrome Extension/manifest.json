{
  "manifest_version": 2,

  "name": "SuperIcognito",
  "description": "Scrambles up text and makes most things barely legible",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [activeTab]
}
